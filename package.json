    "ltx": "^2.2.0",
    "node-xmpp-client": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.13",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "nodemon": "^1.8.1"
  },
  "repository": {
    "type": "git",
